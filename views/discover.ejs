 <!--Modal window of Contact Us button -->
	<div class="modal" tabindex="-1" role="dialog" id="myModal">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title">Contact Us</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <form class="email-request-form">
          <div class="form-group">
    		<label for="name">Your name</label>
   			<input type="text" class="form-control" id="name" placeholder="Name">
  		  </div>
  		  <div class="form-group">
    		<label for="email">Email</label>
   			<input type="text" class="form-control" id="email" placeholder="Email">
  		  </div>
  		  <div class="form-group">
   			 <label for="message">Message</label>
    		<textarea class="form-control" id="message" rows="3"></textarea>
 		  </div>
 		  <input type="submit" class="btn btn-primary" data-toggle="modal" data-target="#myModal">
        </form>
      </div>
      
    </div>
  </div>
</div>

<div id="call">
    
</div>

<footer>
    <h2>Subscribe & <br>get daily updates</h2>
    <p>After subscription you will get daily updates of trending destinations.</p>
    <div class="input">
        <form class="call-me-form">
            <input type="text" placeholder="Phone number" aria-label="Search">
            <button type="submit">Call me</button>
          </form>
    </div>
    <div class="input"  style="margin-top: 10px;">
        <form class="sub-form">
            <input type="text" placeholder="Email" aria-label="Search" id="email">
            <button type="submit">Subscribe</button>
          </form>
    </div>
    <div class="col-2 d-flex justify-content-end align-items-center" >
        <button class="btn btn-primary" id="contactUs" data-toggle="modal" data-target="#myModal" style="text-wrap: nowrap; margin:10px;">Contact Us</button>
        <a href="https://docs.google.com/forms/d/e/1FAIpQLSdeSmGCOlInBTlybifUk6a3i_gSlbYS_gLkT0yIcP2pBV9pyw/viewform?usp=sf_link"><button class="btn btn-primary" style="text-wrap: nowrap; margin:10px;">Feedback</button></a>
    </div>
    <ul>
        <li>12<br><h6>Year Serving the travel Industry</h6></li>
        <li>964<br><h6>India Based<br>Partnership</h6></li>
        <li>14<br><h6>Industry Awards since 2011</h6></li>
        <li>6340<br><h6>Subscribed</h6></li>
    </ul>
</footer>
